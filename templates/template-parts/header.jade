#login-modal.custom-modal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')

	.modal-dialog(role='document')
	
		.modal-content
		
			.modal-header
			
				button.close(type='button', data-dismiss='modal', aria-label='Close')
					img(src="assets/images/close.png")
				
			.custom-modal__body
			
				h4.custom-modal__title Авторизация
				
				form(method="" action="#" id="" name="")
				
					.form-group
						
						input.form-control(type="email"  placeholder="E-mail"  name="")
						
					.form-group
						
						input.form-control(type="password"  placeholder="Пароль"  name="")
					
					
						
					button.btn.btn-lg.btn-primary(type="submit") Войти
					
					.registration-group
					
						a.registration-link(href="") Регистрация
						
					.item-forgout
					
						label.custom-control.custom-checkbox
							input.custom-control-input(type='checkbox')
							span.custom-control-indicator
							span.custom-control-description Запомнить
							
						a.forgout(href="#") 
							span Напомнить пароль 
					
			
			
			.social-modal
				
				.block-share
				
					p Войти через социальные сети:
					
					ul 
					
						li
						
							a(href="#")
							
								i.vk
								
						li
						
							a(href="#")
							
								i.fb
								
						li
						
							a(href="#")
							
								i.tw
								
						li
						
							a(href="#")
							
								i.ok
									
							
			
