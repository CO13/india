#click.custom-modal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')

	.modal-dialog(role='document')
	
		.modal-content
		
			.modal-header
				
				button.close(type='button', data-dismiss='modal', aria-label='Close')
					img(src="assets/images/close.png")
					
			.custom-modal__body
			
				h4.custom-modal__title Купить в 1 клик
				
				p.custom-modal__desc-blue Hepcinat LP Соф+ледипасвир , 90 табл.
				
				p.custom-modal__desc. 
					Оставьте Ваш телефон и в течении 10 минут наш менеджер перезвонит Вам
					
				form(method="" action="#" id="" name="")
				
					.form-group
						
						input.form-control(type="text"  placeholder="Ваше имя" id="title-review" name="title-review")
						
					.form-group
						
						
						input.form-control(type="text"  placeholder="Телефон" id="title-review" name="title-review")
						
					.qty-wrap 
					
						span Количество:
						
						span.qty
						
							button.button-down(type='button', onclick='changeQty(12, this);')
							
							input#cart-qty-12.qty-input(type="text" value='1', size='4', title='Qty', name='')
							
							button.button-up(type='button', onclick='changeQty(12, this);')
							
					button.btn.btn-lg.btn-primary(type="submit") Заказать товар

